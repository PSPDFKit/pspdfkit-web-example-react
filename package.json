{
  "name": "pspdfkit-web-example-react",
  "version": "2018.2.0",
  "description": "Standalone and offline PDF viewer web application built with PSPDFKit for Web and React",
  "keywords": [
    "wasm",
    "web",
    "assembly",
    "pdf",
    "pdf",
    "viewer",
    "react",
    "asm.js",
    "offline",
    "indexeddb"
  ],
  "author": "PSPDFKit (https://pspdfkit.com)",
  "homepage": "https://pspdfkit.com/web",
  "license": "SEE LICENSE IN https://pspdfkit.com/license.pdf",
  "readme": "https://pspdfkit.com/guides/web",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "do-prepare": "node scripts/verify-install.js && node scripts/copy-pspdfkit-files.js",
    "prestart": "npm run do-prepare",
    "start": "cross-env REACT_APP_PSPDFKIT_KEY='YOUR_LICENSE_KEY_GOES_HERE' react-scripts start",
    "prebuild": "npm run do-prepare",
    "build": "cross-env REACT_APP_PSPDFKIT_KEY='YOUR_LICENSE_KEY_GOES_HERE' react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "cross-env": "^5.1.4"
  },
  "resolutions": {
    "mime": "^1.6.0"
  }
}
